    "@biomejs/biome": "^2.0.6",
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@types/node": "^24.0.10",
    "@types/xml2js": "^0.4.14",
    "esbuild": "^0.25.5",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.3",
